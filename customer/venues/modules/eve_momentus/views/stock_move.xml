<odoo>
    <record id="view_stock_move_form_inherit_momentus" model="ir.ui.view">
        <field name="name">stock.move.form.inherit.momentus</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='origin_grp']/field[@name='origin']" position="after">
                <field name="momentus_wo"/>
            </xpath>
        </field>
    </record>
    <record id="view_stock_move_tree_inherit_momentus" model="ir.ui.view">
        <field name="name">stock.move.list.inherit.momentus</field>
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree"/>
        <field name="arch" type="xml">
            <xpath expr="field[@name='date']" position="after">
                <field name="momentus_wo"/>
                <field name="origin"/>
            </xpath>
        </field>
    </record>


</odoo>