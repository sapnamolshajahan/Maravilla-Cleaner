<odoo>
    <record id="ir_cron_check_next_fire_drill" model="ir.cron">
        <field name="name">Check Next Fire Drill Due</field>
        <field name="model_id" ref="model_fire_drills"/>
        <field name="state">code</field>
        <field name="code">model._check_next_fire_drill_due()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active">False</field>
    </record>
</odoo>
