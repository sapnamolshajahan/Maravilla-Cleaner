<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="rebate_report_xls_export_options_view">
        <field name="name">rebate.report.xls_export_options_view</field>
        <field name="model">rebate.report.xls_export_options</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Export Options">
                <group colspan="2">
                    <field name="report_name" invisible="1"/>
                    <field name="export_xls" filename="report_name"/>
                </group>
                <footer>
                    <button class="oe_link" special="cancel" string="Close"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
