<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_warehouse_form_view_inherit_stock_xero" model="ir.ui.view">
            <field name="name">stock.warehouse.form.inherit.stock.xero</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="after">
                    <field name="default_analytic_account" options="{'no_create_edit': True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
