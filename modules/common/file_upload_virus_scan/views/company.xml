<?xml version="1.0"?>
<odoo>
	<data>
		<record id="extended_company_form" model="ir.ui.view">
			<field name="name">Extended Company Form</field>
			<field name="model">res.company</field>
			<field name="inherit_id" ref="base.view_company_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='currency_id']" position="after">
					<field name="allowed_mimetypes"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>
