<odoo>
    <record id="form_linkly_wizard_message" model="ir.ui.view">
        <field name="name">EFTPOS Message wizard</field>
        <field name="model">linkly.wizard.message</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Message">
                <field name="text" nolabel="1" readonly="1" widget="html" />
                <footer>
                    <button special="cancel" class="oe_highlight" string="Close" />
                </footer>
            </form>
        </field>
    </record>
</odoo>